generator client {
  provider = "prisma-client-py"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}
model Student {
  id         Int      @id @default(autoincrement())
  first_name       String
  email      String   @unique
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("Student_Table")
}